{"@type":"KeepsGroup","title":"S3","icon":null,"keepsNodes":[{"@type":"Keep","title":"bucket - create","icon":null,"phrase":"aws s3 mb s3://<bucket-name-input>","description":"Creates a bucket","parameters":[{"name":"bucket-name-input","phrase":"","global":false}]},{"@type":"Keep","title":"bucket - delete","icon":null,"phrase":"aws s3 rb s3://<bucket-name>","description":"Will delete a S3 bucket.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":false}]},{"@type":"Keep","title":"bucket - list","icon":null,"phrase":"aws s3 ls","description":"Will list all the S3 buckets.","parameters":[]},{"@type":"Keep","title":"bucket - list objects under a bucket","icon":null,"phrase":"aws s3 ls s3://<bucket-name> --recursive","description":"Listing Objects in a Bucket.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true}]},{"@type":"Keep","title":"delete an object","icon":null,"phrase":"aws s3 rm s3://<bucket-name>/<s3-object-name>","description":"Will delete an object under a bucket.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"s3-object-name","phrase":"aws s3api list-objects-v2 --bucket <bucket-name> --query 'Contents[].Key'","global":false}]},{"@type":"Keep","title":"download directory from a bucket","icon":null,"phrase":"aws s3 cp s3://<bucket-name>/<s3-folder-name> <local-folder-name> --recursive","description":"Will copy a directory from a bucket to the local machine.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"s3-folder-name","phrase":"aws s3api list-objects-v2 --bucket <bucket-name-command> --delimiter \"/\" --query 'CommonPrefixes[].Prefix' --output text","global":false},{"name":"local-folder-name","phrase":"","global":false}]},{"@type":"Keep","title":"download file from a bucket","icon":null,"phrase":"aws s3 cp s3://<bucket-name>/<s3-file-name> <local-file-name>","description":"Will copy a file from S3 to the local machine.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"s3-file-name","phrase":"aws s3api list-objects-v2 --bucket <bucket-name> --query 'Contents[].Key'","global":false},{"name":"local-file-name","phrase":"","global":false}]},{"@type":"Keep","title":"sync files between local and remote","icon":null,"phrase":"aws s3 sync <local-folder-name> s3://<bucket-name>/<s3-folder-name>","description":"WIll sync the files between a local and remote folders.","parameters":[{"name":"bucket-name","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"local-folder-name","phrase":"","global":false},{"name":"s3-folder-name","phrase":"aws s3api list-objects-v2 --bucket <bucket-name> --delimiter \"/\" --query 'CommonPrefixes[].Prefix' --output text","global":false}]},{"@type":"Keep","title":"upload directory to a bucket","icon":null,"phrase":"aws s3 cp <directory-name> s3://<bucket-name-command>/<directory-name> --recursive","description":"Will copy a complete directory to an S3 bucket.","parameters":[{"name":"bucket-name-command","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"directory-name","phrase":"","global":false}]},{"@type":"Keep","title":"upload file to a bucket","icon":null,"phrase":"aws s3 cp <input-file> s3://<bucket-name-command>","description":"Will copy a local file to S3 bucket","parameters":[{"name":"bucket-name-command","phrase":"aws s3api list-buckets --query 'Buckets[*].Name'","global":true},{"name":"input-file","phrase":"","global":false}]}]}